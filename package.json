{
    "name": "git-difftool",
    "displayName": "git-difftool",
    "description": "run git difftool",
    "version": "0.0.1",
    "publisher": "elppa",
    "icon": "image/logo@2x.png",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.runGitDifftool"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.runGitDifftool",
            "title": "Run Difftool"
        }],
        "menus": {
            "scm/resourceState/context": [{
                "command": "extension.runGitDifftool"
            }]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}